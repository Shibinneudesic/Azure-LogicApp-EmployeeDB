{
  "Employees": [
    {% for e in content.Employees.* %}
      {
        "EMPLOYEE_ID": "{{ e.EMPLOYEE_ID | default: '' }}",
        "FIRST_NAME": "{{ e.FIRST_NAME | default: '' }}",
        "LAST_NAME": "{{ e.LAST_NAME | default: '' }}",
        "EMAIL": "{{ e.EMAIL | default: '' }}",
        "PHONE_NUMBER": "{{ e.PHONE_NUMBER | default: '' }}",
        "HIRE_DATE": "{{ e.HIRE_DATE | default: '' }}",
        "JOB_ID": "{{ e.JOB_ID | default: '' }}",
        "SALARY": "{{ e.SALARY | default: '' }}",
        "COMMISSION_PCT": "{{ e.COMMISSION_PCT | default: '' }}",
        "MANAGER_ID": "{{ e.MANAGER_ID | default: '' }}",
        "DEPARTMENT_ID": "{{ e.DEPARTMENT_ID | default: '' }}"
      }
      {% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}